FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.63; // Your calculated k

boundaryField
{
    minX // Inlet
    {
        type            atmBoundaryLayerInletK; // Auto-calculates profile
        Uref            10.0;
        Zref            50.0;
        z0              uniform 0.5;
        flowDir         (1 0 0);
        value           uniform 2.63;
    }
    maxX
    {
        type            inletOutlet;
        inletValue      uniform 2.63;
        value           uniform 2.63;
    }
    ground
    {
        type            kqRWallFunction; // Wall function for ground
        value           uniform 2.63;
    }
    buildings
    {
        type            kqRWallFunction;
        value           uniform 2.63;
    }
    top
    {
        type            zeroGradient;
    }
    sides
    {
        type            zeroGradient;
    }
}
